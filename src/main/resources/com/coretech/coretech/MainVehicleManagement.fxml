<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<?import javafx.scene.layout.StackPane?>
<BorderPane prefWidth="800" prefHeight="600" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.coretech.coretech.MainVehicleController">
    <!-- Top Bar -->
    <top>
        <HBox alignment="CENTER" style="-fx-background-color: #2293C3; -fx-padding: 10;" HBox.hgrow="ALWAYS">
            <Label alignment="CENTER" maxWidth="Infinity" style="-fx-font-size: 25px; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-family: Inder;" text="Manage Vehicle" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
        </HBox>
    </top>

    <!-- Left Sidebar -->
    <left>
        <VBox minWidth="150.0" maxWidth="250.0" spacing="12" style="-fx-background-color: white; -fx-border-color: #2293C3;" VBox.vgrow="ALWAYS">
            <ImageView fitHeight="72.0" fitWidth="143.0" preserveRatio="true">
                <Image url="@../../../Images/BlackVersion.png" />
                <VBox.margin>
                    <Insets left="30.0" />
                </VBox.margin>
                <effect>
                    <DropShadow offsetY="2.0">
                        <color>
                            <Color opacity="0.5896656513214111" />
                        </color>
                    </DropShadow>
                </effect>
            </ImageView>
            <Label style="-fx-font-size: 14px; -fx-text-fill: black;" text="Welcome, Name">
                <VBox.margin>
                    <Insets bottom="30.0" left="50.0" />
                </VBox.margin>
            </Label>

            <!-- Home Button -->
            <Button onAction="#handleHome" style="-fx-background-color: white; -fx-text-fill: #000000; -fx-alignment: center-left;" text="Home" maxWidth="Infinity" HBox.hgrow="ALWAYS">
                <graphic>
                    <HBox alignment="CENTER_LEFT" spacing="10"> <!-- Add spacing between icon and text -->
                        <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                            <Image url="@../../../Images/icons/home.png" />
                            <HBox.margin>
                                <Insets right="10.0" /> <!-- Add right padding to the icon -->
                            </HBox.margin>
                        </ImageView>
                    </HBox>
                </graphic>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </Button>

            <!-- Customer Management Button -->
            <Button onAction="#handleCustomerManagement" style="-fx-background-color: #FFFFFF; -fx-text-fill: #000000; -fx-alignment: center-left;" text="Customer Management" maxWidth="Infinity" HBox.hgrow="ALWAYS">
                <graphic>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                            <Image url="@../../../Images/icons/people.png" />
                            <HBox.margin>
                                <Insets right="10.0" /> <!-- Add right padding to the icon -->
                            </HBox.margin>
                        </ImageView>
                    </HBox>
                </graphic>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </Button>

            <!-- Vehicle Management Button -->
            <Button onAction="#handleVehicleManagement" style="-fx-background-color: #A7DAF8; -fx-text-fill: #000000; -fx-alignment: center-left;" text="Vehicle Management" maxWidth="Infinity" HBox.hgrow="ALWAYS">
                <graphic>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                            <Image url="@../../../Images/icons/car.png" />
                            <HBox.margin>
                                <Insets right="10.0" /> <!-- Add right padding to the icon -->
                            </HBox.margin>
                        </ImageView>
                    </HBox>
                </graphic>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </Button>

            <!-- Appointments Button -->
            <Button onAction="#handleAppointments" style="-fx-background-color: #FFFFFF; -fx-text-fill: #000000; -fx-alignment: center-left;" text="Appointments" maxWidth="Infinity" HBox.hgrow="ALWAYS">
                <graphic>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                            <Image url="@../../../Images/icons/appointment.png" />
                            <HBox.margin>
                                <Insets right="10.0" /> <!-- Add right padding to the icon -->
                            </HBox.margin>
                        </ImageView>
                    </HBox>
                </graphic>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </Button>

            <!-- Spacer and Servicing Button -->
            <VBox fx:id="spacer" VBox.vgrow="ALWAYS">
                <children>
                    <Button onAction="#handleServicing" style="-fx-background-color: #FFFFFF; -fx-text-fill: #000000; -fx-alignment: center-left;" text="Servicing" maxWidth="Infinity" HBox.hgrow="ALWAYS">
                        <graphic>
                            <HBox alignment="CENTER_LEFT" spacing="10">
                                <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                                    <Image url="@../../../Images/icons/service.png" />
                                    <HBox.margin>
                                        <Insets right="10.0" /> <!-- Add right padding to the icon -->
                                    </HBox.margin>
                                </ImageView>
                            </HBox>
                        </graphic>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                    </Button>
                </children>
            </VBox>

            <!-- Log Out Button -->
            <Button onAction="#handleLogout" style="-fx-background-color: #2293C3; -fx-text-fill: white; -fx-font-weight: bold;" text="Log Out" maxWidth="Infinity" HBox.hgrow="ALWAYS">
                <graphic>
                    <HBox alignment="CENTER_LEFT" spacing="10"> <!-- Add spacing between icon and text -->
                        <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                            <Image url="@../../../Images/icons/logout-white.png" />
                            <HBox.margin>
                                <Insets right="10.0" /> <!-- Add right padding to the icon -->
                            </HBox.margin>
                        </ImageView>
                    </HBox>
                </graphic>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </Button>
        </VBox>
    </left>

    <!-- Main Content -->
    <center>
        <VBox fx:id="centerPane" alignment="CENTER" spacing="30" style="-fx-padding: 30; -fx-background-color: #EFEFEF; -fx-border-color: #2293C3;" VBox.vgrow="ALWAYS">
            <!-- Add Vehicle Form -->
            <VBox fx:id="addForm" alignment="CENTER" spacing="25" style="-fx-padding: 30; -fx-background-color: #EFEFEF;" visible="false" managed="false" VBox.vgrow="ALWAYS">
                <Label style="-fx-font-size: 24px; -fx-font-weight: bold; -fx-text-fill: #2293C3;" text="Add Vehicle" />

                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 16px; -fx-text-fill: black; -fx-min-width: 120;" text="Email:" />
                    <TextField fx:id="addEmailField" prefWidth="250.0" promptText="Enter Email" style="-fx-font-size: 14px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 16px; -fx-text-fill: black; -fx-min-width: 120;" text="Make:" />
                    <TextField fx:id="addMake" prefWidth="250.0" promptText="Enter Make" style="-fx-font-size: 14px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 16px; -fx-text-fill: black; -fx-min-width: 120;" text="Model:" />
                    <TextField fx:id="addModel" prefWidth="250.0" promptText="Enter Model" style="-fx-font-size: 14px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 16px; -fx-text-fill: black; -fx-min-width: 120;" text="Year:" />
                    <TextField fx:id="addYear" prefWidth="250.0" promptText="Enter Year" style="-fx-font-size: 14px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 16px; -fx-text-fill: black; -fx-min-width: 120;" text="VIN:" />
                    <TextField fx:id="addVin" prefWidth="250.0" promptText="Enter VIN" style="-fx-font-size: 14px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 16px; -fx-text-fill: black; -fx-min-width: 120;" text="Service History:" />
                    <TextField fx:id="addServiceHistory" prefWidth="250.0" promptText="Enter Service History" style="-fx-min-height: 70; -fx-font-size: 14px;" />
                </HBox>

                <HBox alignment="CENTER" spacing="40" style="-fx-padding: 10;">
                    <!-- Add Button with Icon -->
                    <Button fx:id="addButton" style="-fx-background-color: #198B2C; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 16px; -fx-pref-width: 120; -fx-pref-height: 40;" text="ADD">
                        <graphic>
                            <HBox alignment="CENTER_LEFT" spacing="10"> <!-- Add spacing between icon and text -->
                                <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                                    <Image url="@../../../Images/icons/add-white.png" /> <!-- Path to your add icon -->
                                    <HBox.margin>
                                        <Insets right="10.0" /> <!-- Add right padding to the icon -->
                                    </HBox.margin>
                                </ImageView>
                            </HBox>
                        </graphic>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                    </Button>

                    <Button fx:id="cancelAddButton" style="-fx-background-color: #2293C3; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 16px; -fx-pref-width: 120; -fx-pref-height: 40;" text="CANCEL">
                        <graphic>
                            <HBox alignment="CENTER_LEFT" spacing="10"> <!-- Add spacing between icon and text -->
                                <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                                    <Image url="@../../../Images/icons/cancle.png" /> <!-- Path to your cancel icon -->
                                    <HBox.margin>
                                        <Insets right="10.0" /> <!-- Add right padding to the icon -->
                                    </HBox.margin>
                                </ImageView>
                            </HBox>
                        </graphic>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                    </Button>
                </HBox>
            </VBox>

            <!-- Update Vehicle Form -->
            <VBox fx:id="updateForm" alignment="CENTER" spacing="25" style="-fx-padding: 30; -fx-background-color: #EFEFEF;" visible="false" managed="false" VBox.vgrow="ALWAYS">
                <Label style="-fx-font-size: 24px; -fx-font-weight: bold; -fx-text-fill: #2293C3;" text="Update Vehicle" />
                <HBox alignment="CENTER" spacing="10" HBox.hgrow="ALWAYS">
                    <TextField fx:id="updateVinSearchField" prefWidth="250.0" promptText="Enter VIN" style="-fx-font-size: 12px;" />
                    <Button fx:id="updateSearchButton" style="-fx-background-color: #2293C3; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 12px; -fx-pref-width: 80; -fx-pref-height: 30;" text="SEARCH" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Email:" />
                    <TextField fx:id="updateEmailField" prefWidth="250.0" promptText="Enter Email" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Make:" />
                    <TextField fx:id="updateMakeField" prefWidth="250.0" promptText="Enter Make" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Model:" />
                    <TextField fx:id="updateModelField" prefWidth="250.0" promptText="Enter Model" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Year:" />
                    <TextField fx:id="updateYearField" prefWidth="250.0" promptText="Enter Year" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="VIN:" />
                    <TextField fx:id="updateVinField" prefWidth="250.0" promptText="Enter VIN" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Service History:" />
                    <TextField fx:id="updateServiceHistoryField" prefWidth="250.0" promptText="Enter Service History" style="-fx-min-height: 70; -fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="40" style="-fx-padding: 10;">

                    <Button fx:id="updateButton" style="-fx-background-color: #198B2C; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 14px; -fx-pref-width: 120; -fx-pref-height: 40;" text="UPDATE">
                        <graphic>
                            <HBox alignment="CENTER_LEFT" spacing="10"> <!-- Add spacing between icon and text -->
                                <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                                    <Image url="@../../../Images/icons/update.png" /> <!-- Path to your update icon -->
                                    <HBox.margin>
                                        <Insets right="10.0" /> <!-- Add right padding to the icon -->
                                    </HBox.margin>
                                </ImageView>
                            </HBox>
                        </graphic>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                    </Button>

                    <Button fx:id="updateCancelButton" style="-fx-background-color: #2293C3; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 14px; -fx-pref-width: 120; -fx-pref-height: 40;" text="CANCEL">
                        <graphic>
                            <HBox alignment="CENTER_LEFT" spacing="10"> <!-- Add spacing between icon and text -->
                                <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                                    <Image url="@../../../Images/icons/cancle.png" /> <!-- Path to your cancel icon -->
                                    <HBox.margin>
                                        <Insets right="10.0" /> <!-- Add right padding to the icon -->
                                    </HBox.margin>
                                </ImageView>
                            </HBox>
                        </graphic>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                    </Button>
                </HBox>
            </VBox>

            <!-- Remove Vehicle Form -->
            <VBox fx:id="removeForm" alignment="CENTER" spacing="25" style="-fx-padding: 30; -fx-background-color: #EFEFEF;" visible="false" managed="false" VBox.vgrow="ALWAYS">
                <Label style="-fx-font-size: 24px; -fx-font-weight: bold; -fx-text-fill: #2293C3;" text="Remove Vehicle" />
                <HBox alignment="CENTER" spacing="10" HBox.hgrow="ALWAYS">
                    <TextField fx:id="removeVinSearchField" prefWidth="250.0" promptText="Enter VIN" style="-fx-font-size: 12px;" />
                    <Button fx:id="removeSearchButton" style="-fx-background-color: #2293C3; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 12px; -fx-pref-width: 80; -fx-pref-height: 30;" text="SEARCH" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Email:" />
                    <TextField fx:id="removeEmailField" prefWidth="250.0" promptText="Enter Email" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Make:" />
                    <TextField fx:id="removeMakeField" prefWidth="250.0" promptText="Enter Make" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Model:" />
                    <TextField fx:id="removeModelField" prefWidth="250.0" promptText="Enter Model" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Year:" />
                    <TextField fx:id="removeYearField" prefWidth="250.0" promptText="Enter Year" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="VIN:" />
                    <TextField fx:id="removeVinField" prefWidth="250.0" promptText="Enter VIN" style="-fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="20" HBox.hgrow="ALWAYS">
                    <Label style="-fx-font-size: 14px; -fx-text-fill: black; -fx-min-width: 120;" text="Service History:" />
                    <TextField fx:id="removeServiceHistoryField" prefWidth="250.0" promptText="Enter Service History" style="-fx-min-height: 70; -fx-font-size: 12px;" />
                </HBox>
                <HBox alignment="CENTER" spacing="40" style="-fx-padding: 10;">

                    <Button fx:id="removeButton" style="-fx-background-color: #D32F2F; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 14px; -fx-pref-width: 120; -fx-pref-height: 40;" text="REMOVE">
                        <graphic>
                            <HBox alignment="CENTER_LEFT" spacing="10"> <!-- Add spacing between icon and text -->
                                <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                                    <Image url="@../../../Images/icons/remove.png" /> <!-- Path to your remove icon -->
                                    <HBox.margin>
                                        <Insets right="10.0" /> <!-- Add right padding to the icon -->
                                    </HBox.margin>
                                </ImageView>
                            </HBox>
                        </graphic>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                    </Button>

                    <Button fx:id="removeCancelButton" style="-fx-background-color: #2293C3; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 14px; -fx-pref-width: 120; -fx-pref-height: 40;" text="CANCEL">
                        <graphic>
                            <HBox alignment="CENTER_LEFT" spacing="10"> <!-- Add spacing between icon and text -->
                                <ImageView fitHeight="20.0" fitWidth="20.0" preserveRatio="true">
                                    <Image url="@../../../Images/icons/cancle.png" /> <!-- Path to your cancel icon -->
                                    <HBox.margin>
                                        <Insets right="10.0" /> <!-- Add right padding to the icon -->
                                    </HBox.margin>
                                </ImageView>
                            </HBox>
                        </graphic>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                    </Button>
                </HBox>
            </VBox>
            <!-- Servicing Details Form -->
            <VBox fx:id="servicingForm" alignment="CENTER" spacing="30" style="-fx-padding: 30; -fx-background-color: #EFEFEF;" visible="false" managed="false" VBox.vgrow="ALWAYS">
                <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="Infinity" style="-fx-font-size: 20px; -fx-font-weight: bold; -fx-text-fill: #2293C3;" text="Servicing Details" textAlignment="CENTER" />

                <!-- VIN Search -->
                <HBox alignment="CENTER" spacing="10">
                    <TextField fx:id="vinTextField" prefHeight="30.0" promptText="Enter VIN" style="-fx-font-size: 12px; -fx-pref-width: 250; -fx-max-width: 300;" HBox.hgrow="ALWAYS">
                        <HBox.margin>
                            <Insets />
                        </HBox.margin>
                    </TextField>
                    <Button onAction="#handleSearch" style="-fx-background-color: #2293C3; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 12px; -fx-pref-width: 80; -fx-pref-height: 30;" text="SEARCH" />
                    <VBox.margin>
                        <Insets bottom="20.0" />
                    </VBox.margin>
                </HBox>

                <!-- Service History Table -->
                <VBox alignment="CENTER" style="-fx-padding: 10; -fx-background-color: #EFEFEF; -fx-border-color: #EFEFEF;" VBox.vgrow="ALWAYS">
                    <TableView fx:id="serviceHistoryTable" prefHeight="345.0" prefWidth="413.0" style="-fx-border-color: black; -fx-border-width: 1px;">
                        <columns>
                            <TableColumn fx:id="dateColumn" prefWidth="117.59998154640198" style="-fx-alignment: CENTER; -fx-background-color: #2293C3; -fx-text-fill: white; -fx-font-weight: bold; -fx-border-color: black; -fx-border-width: 1px;" text="Date" />
                            <TableColumn fx:id="descriptionColumn" prefWidth="292.8" style="-fx-alignment: CENTER; -fx-background-color: #2293C3; -fx-text-fill: white; -fx-font-weight: bold; -fx-border-color: black; -fx-border-width: 1px;" text="Description" />
                        </columns>
                    </TableView>
                </VBox>
            </VBox>
        </VBox>
    </center>

    <!-- Right Panel -->
    <right>
        <AnchorPane minWidth="200.0" maxWidth="350.0" style="-fx-border-color: #2293C3;" VBox.vgrow="ALWAYS">
            <Button fx:id="addVehicleButton" onAction="#handleAddVehicle" style="-fx-background-color: white; -fx-text-fill: #2293C3; -fx-border-color: #2293C3;" text="Add Vehicle" AnchorPane.topAnchor="170.0" AnchorPane.leftAnchor="4.0" AnchorPane.rightAnchor="4.0" />
            <Button fx:id="removeVehicleButton" onAction="#handleRemoveVehicle" style="-fx-background-color: white; -fx-text-fill: #2293C3; -fx-border-color: #2293C3;" text="Remove Vehicle" AnchorPane.topAnchor="233.0" AnchorPane.leftAnchor="4.0" AnchorPane.rightAnchor="4.0" />
            <Button fx:id="updateVehicleButton" onAction="#handleUpdateVehicle" style="-fx-background-color: white; -fx-text-fill: #2293C3; -fx-border-color: #2293C3;" text="Update Vehicle Info" AnchorPane.topAnchor="297.0" AnchorPane.leftAnchor="4.0" AnchorPane.rightAnchor="4.0" />
            <Button fx:id="servicingDetailsButton" onAction="#handleServicingDetails" style="-fx-background-color: white; -fx-text-fill: #2293C3; -fx-border-color: #2293C3;" text="Servicing Details" AnchorPane.topAnchor="363.0" AnchorPane.leftAnchor="4.0" AnchorPane.rightAnchor="4.0" />
        </AnchorPane>
    </right>
</BorderPane>